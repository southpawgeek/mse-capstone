{{>header}}
    <section>
    <h2>Browse Titles</h2>
    <p id="message" style="display:none;"></p>
    <h3>Search <i class="fas fa-search"></i></h3>
    <input name="search" id="search" type="text" />
    </section>
    <section>
        <h2>Titles</h2>
        <table id="assets">
            <thead>
                <th>Title</th>
                <th>ISBN</th>
                <th>Call Number</th>
                <th>Available</th>
            </thead>
            <tbody>
                {{#assets}}<tr>
                <td>{{title}}</td>
                <td>{{isbn}}</td>
                <td>{{callNumber}}</td>
                <td>?</td>
                {{/assets}}</tr>
            </tbody>
        </table>
    </section>
{{>footer}}